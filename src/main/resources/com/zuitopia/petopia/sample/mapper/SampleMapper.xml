<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zuitopia.petopia.sample.mapper.SampleMapper">

	<select id="getList" resultType="dto.SampleVO">
		<![CDATA[
			select id, name
			from test
		]]>
	</select>
	
	<select id="count" >
		<![CDATA[
			select count(*)
			from test
		]]>
	</select>

	<select id="getUser" >
		<![CDATA[
		select count(*)
		from USERS
		where  user_email = #{userEmail}
		]]>
	</select>
</mapper>

